{
    "name":"VocalizeIt",
    "manifest_version":3,
    "version":"1.0.0",
    "author": "Eric Worsfold",
    "action": {
        "default_popup": "index.html",
        "default_title": "Read articals with Ai"
    },
    "content_scripts": [
        {
            "matches":["<all_urls>"],
            "js":["popup.js"],
            "css":["style.css"]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": ["/images/test.jpg"],
            "matches": ["https://twitter.com/*"]
        }
    ],
    "permissions": [
        "sidePanel"
    ]
}
